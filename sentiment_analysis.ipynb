    " def calculate_purity(labels_true, labels_pred):\n",
    "        label_map_true = {label: idx for idx, label in enumerate(np.unique(labels_true))}\n",
    "        label_map_pred = {label: idx for idx, label in enumerate(np.unique(labels_pred))}\n",
    "        labels_true_int = np.array([label_map_true[label] for label in labels_true])\n",
    "        labels_pred_int = np.array([label_map_pred[label] for label in labels_pred])\n",
    "\n",
    "        n = len(labels_true)\n",
    "        k = len(np.unique(labels_true))\n",
    "        contingency_matrix = np.zeros((k, k))\n",
    "        for i in range(n):\n",
    "            contingency_matrix[labels_true_int[i], labels_pred_int[i]] += 1\n",
    "\n",
    "        purity = np.sum(np.amax(contingency_matrix, axis=0)) / n\n",
    "        return purity"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "def calculate_nmi(labels_true, labels_pred):\n",
    "    label_map_true = {label: idx for idx, label in enumerate(np.unique(labels_true))}\n",
    "    label_map_pred = {label: idx for idx, label in enumerate(np.unique(labels_pred))}\n",
    "    labels_true_int = np.array([label_map_true[label] for label in labels_true])\n",
    "    labels_pred_int = np.array([label_map_pred[label] for label in labels_pred])\n",
    "\n",
    "    n = len(labels_true_int)\n",
    "    k = len(np.unique(labels_true_int))\n",
    "    contingency_matrix = np.zeros((k, k))\n",
    "    for i in range(n):\n",
    "        contingency_matrix[labels_true_int[i], labels_pred_int[i]] += 1\n",
    "\n",
    "    contingency_matrix = contingency_matrix / n\n",
    "    p_i = np.sum(contingency_matrix, axis=1)\n",
    "    p_j = np.sum(contingency_matrix, axis=0)\n",
    "    outer = np.outer(p_i, p_j)\n",
    "    eps = np.finfo(float).eps\n",
    "    outer[outer < eps] = eps\n",
    "    mi = contingency_matrix * (np.log((contingency_matrix + eps) / outer))\n",
    "    mi = np.sum(mi)\n",
    "\n",
    "    h_true = -np.sum(p_i * np.log(p_i))\n",
    "    h_pred = -np.sum(p_j * np.log(p_j))\n",
    "\n",
    "    nmi = mi / np.sqrt(h_true * h_pred)\n",
    "    return nmi\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "def calculate_rand_score(labels_true, labels_pred):\n",
    "    label_map_true = {label: idx for idx, label in enumerate(np.unique(labels_true))}\n",
    "    label_map_pred = {label: idx for idx, label in enumerate(np.unique(labels_pred))}\n",
    "    labels_true_int = np.array([label_map_true[label] for label in labels_true])\n",
    "    labels_pred_int = np.array([label_map_pred[label] for label in labels_pred])\n",
    "\n",
    "    n = len(labels_true_int)\n",
    "    k = len(np.unique(labels_true_int))\n",
    "    contingency_matrix = np.zeros((k, k))\n",
    "    for i in range(n):\n",
    "        contingency_matrix[labels_true_int[i], labels_pred_int[i]] += 1\n",
    "\n",
    "    a = contingency_matrix.sum(axis=0)\n",
    "    b = contingency_matrix.sum(axis=1)\n",
    "    tp = np.sum(contingency_matrix * (contingency_matrix - 1)) / 2\n",
    "    fp = np.sum(a * (a - 1)) / 2 - tp\n",
    "    fn = np.sum(b * (b - 1)) / 2 - tp\n",
    "    tn = n * (n - 1) / 2 - tp - fp - fn\n",
    "    rand_score = (tp + tn) / (tp + fp + fn + tn)\n",
    "    return rand_score\n"
   ]
